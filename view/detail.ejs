</div>
<script src="/js/jquery.min.js"></script>
<script>
    $(function(){
        var T = null;
        var T2 = null;
        $(".zyq_btn").on('mouseenter',function(){
            clearInterval(T2);
            clearInterval(T);
            T2 = setInterval(function(){
                $('.ewm').fadeIn();
                $('.zyq_btn').addClass('active');
            },100)  

        })
        $(".zyq_btn").on('mouseleave',function(){
            clearInterval(T);
            clearInterval(T2);
            T = setInterval(function(){
                $('.ewm').fadeOut();
                $('.zyq_btn').removeClass('active');
            },100)
        })
    })
</script>
</script>
  </body>
</html>