<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <meta http-equiv="Expires" CONTENT="0"> 
    <meta http-equiv="Cache-Control" CONTENT="no-cache"> 
    <meta http-equiv="Pragma" CONTENT="no-cache"> 
    <link rel="shortcut icon" href="/images/favicon.ico" type="image/x-icon" />
    <title><%=detail.title%></title>
    <script type="text/javascript"> if(!window.console){ window.console = {}; } if(!window.console.log){ window.console.log = function(msg){}; }</script>
    <link rel="stylesheet" href="/css/index.css" />
</head>
<body>  
    <header id="header2">
        <div class="container clf"> 
            <a class="logo" href="/">
                <img src="/images/logo_header.png" alt="">
            </a>       
            <div class="header_right">
                <a class="" href="http://justsign.com.cn/" class="sign-btn cube" target="_blank"><img src="/images/qb_icon.png" alt=""></a>
                <a href="javascript:;" class="zyq_btn"><img src="/images/zyq_icon.png"  alt="">
                    <div class="ewm">                        
                        <img src="/images/erweima.png" alt="">
                        <h3>真宜签APP下载二维码</h3>
                    </div>
                </a>
            </div>
        </div>
    </header>
<div class="index_detail">
    <div class="container">
        <div class="side_left">
            <div class="title">
                <a href="/">首页</a>><span>新闻详情</span>
            </div>
            <div class="hd">
                <h1><%=detail.title%></h1>
                <span><%= moment(detail.createTime).format('YYYY-MM-DD')%></span>
                <span>来源:<%=detail.source%></span>
            </div>
            <div class="detail_contain">
                <%-detail.content%>
            </div>
        </div>
        <div class="side_right">
            <div class="newsList">
                <h3>新闻咨讯</h3>
                <ul>
                    <% recomendThumbData.forEach(function(item){%>
                        <li>
                            <a href="/detail/<%=item._id%>">
                                <span>
                                    <img src="<%=hostLink+item.img%>" alt="">
                                </span>
                                <p class="shengl"><%=item.title%></p>
                            </a>
                        </li>
                    <% }) %>
                </ul>
            </div>
        </div>
    </div>
    
</div>
<div class="footer2">
