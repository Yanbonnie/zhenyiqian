<script>
    $(function(){
        var width = $(window).width();
        var height = $(window).height();
        var T = null;
        var T2 = null;
        var scale = width/height;
        if(scale > (1920/965)){
            $('.home,#bottom').css("background-size","100% auto");
        }
        $("#wrap-silde").fullpage({
            "navigation": true,
            verticalCentered: false,
            resize: true
        });
        $('body').css('visibility','visible');
        $(".zyq_btn").on('mouseenter',function(){
            clearInterval(T2);
            clearInterval(T);
            T2 = setInterval(function(){
                $('.ewm').fadeIn();
                $('.zyq_btn').addClass('active');
            },100)  

        })
        $(".zyq_btn").on('mouseleave',function(){
            clearInterval(T);
            clearInterval(T2);
            T = setInterval(function(){
                $('.ewm').fadeOut();
                $('.zyq_btn').removeClass('active');
            },100)
        })
        
    })
</script>
